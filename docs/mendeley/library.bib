   keywords = {Adversarial machine learning,Contrastive Learning,Hydropower,Machine learning,Machine learning models,Machine-learning,Mahabad Dam,Mahabad dam,Optimisations,Optimization algorithms,Performance,Prediction models,River flow,Single models,Support vector regression,Support vector regressions,Surface water resources,Surface waters,algorithm,dam,hydroelectric power,inflow,machine learning,model,river flow},
