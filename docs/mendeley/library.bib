   journal = {Acta Geophysica},
   keywords = {Aquifers,Artificial intelligence,Data handling,Flow of water,Flow prediction,Forecasting,Fuzzy inference,Fuzzy neural networks,Fuzzy systems,Gene expression,Groundwater resources,Group method of data handling,Hybrid model,Hybrid models,Hydraulics,Iran,Learning systems,Least squares approximations,Machine learning models,Performance,Potable water,Qanat,Root mean squared errors,Standalone model,Support vector machines,Water flows,Wavelet transforms,Wavelets transform,aquifer,artificial intelligence,machine learning,numerical model,prediction,transform,water flow,wavelet analysis},
