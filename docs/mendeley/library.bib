   issue = {3},
   journal = {Environmental Science and Pollution Research},
   keywords = {ARIMA-LSTM,ARIMA-SVR,China,Drought forecasting,Droughts,Forecasting,LS-SVR,SPEI,drought,environmental modeling,evapotranspiration,forecasting,risk assessment,spatiotemporal analysis},
