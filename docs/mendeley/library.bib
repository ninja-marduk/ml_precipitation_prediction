   keywords = {'current,Additive regression,Additives,Bangladesh,Data driven technique,Drought,Drought prediction,Errors,Evapotranspiration,Forecasting,Hybrid machine learning,Machine learning,Mean absolute error,Mean square error,Northern bangladesh,Random subspaces,Regression analysis,Sensitivity analysis,Standardized precipitation evapotranspiration index,Time series analysis,additive regression,drought prediction,evapotranspiration,hybrid machine learning,machine learning,northern Bangladesh,standardized precipitation evapotranspiration index},
   note = {Export Date: 11 February 2025; Cited By: 11; Correspondence Address: M. Kamruzzaman; Farm Machinery and Postharvest Technology Division, Bangladesh Rice Research Institute, Gazipur, 1701, Bangladesh; email: milonbrri@gmail.com},
