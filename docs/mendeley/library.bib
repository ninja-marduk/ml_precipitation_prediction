   pages = {563-578},
   publisher = {Tarbiat Modares University},
   title = {Comparative evaluation of hybrid sarima and machine learning techniques based on time varying and decomposition of precipitation time series},
   volume = {22},
   url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85081029877&partnerID=40&md5=48ca9aed7c694db8fe2082b939286bab},
