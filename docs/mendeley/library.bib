   issn = {0718-0764},
   issue = {4},
   journal = {Información tecnológica},
   pages = {303-316},
   title = {Modelado y Predicción del Fenómeno El Niño en Piura, Perú mediante Redes Neuronales Artificiales usando Matlab},
   volume = {29},
