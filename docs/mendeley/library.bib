   issn = {09441344 (ISSN)},
   issue = {9},
   journal = {Environmental Science and Pollution Research},
   keywords = {Artificial intelligence,Environmental Monitoring,Groundwater,Groundwater level prediction,Iran,Neural Networks, Computer,Standalone model,Support Vector Machine,Temperature,Wavelet transform,aquifer,artificial diet,artificial neural network,environmental monitoring,ground water,groundwater,machine learning,model,procedures,support vector machine,temperature,wavelet analysis},
   note = {Export Date: 11 February 2025; Cited By: 31; Correspondence Address: M. Vadiati; Global Affairs, Hubert H. Humphrey Fellowship Program, University of California, Davis, 10 College Park, 95616, United States; email: mvadiati@ucdavis.edu; CODEN: ESPLE},
