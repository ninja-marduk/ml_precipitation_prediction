   journal = {Neural Computing and Applications},
   keywords = {Discrete wavelet transforms,Forestry,Gaussian distribution,Gaussian noise (electronic),Gaussian process regression,Hybrid model,Hybrid wavelet-learning machine,Kstar algorithm,Learning algorithms,Learning machines,Machine learning,Machine learning methods,Mean square error,Performance,Precipitation,Random forests,River basins,Watersheds,Wavelet,Weather forecasting},
