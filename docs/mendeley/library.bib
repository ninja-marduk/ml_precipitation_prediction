   note = {Export Date: 11 February 2025; Cited By: 5; Correspondence Address: S. Kale; Department of Fishing and Fish Processing Technology, Cąnakkale Onsekiz Mart University, Cąnakkale, Terzioglu Campus, 17020, Turkey; email: semihkale@comu.edu.tr},
   pages = {354-373},
   publisher = {De Gruyter Open Ltd},
