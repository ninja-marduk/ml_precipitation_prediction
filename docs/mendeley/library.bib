   keywords = {Adaptive noise,Auto-regressive,Autoregressive moving average model,CEEMDAN,Cape Town,Complementary ensemble empirical mode decomposition with adaptive noise,Drought forecasting,Empirical Mode Decomposition,Hybrid model,Mann-Kendall test,Mean square error,Prediction models,Rain,Short term memory,South Africa,Standardized Precipitation Index,Standardized precipitation index,Western Cape,decomposition,drought,drought forecasting,forecasting method,hybrid model,precipitation intensity,time series analysis,trend analysis},
