   volume = {82},
   url = {https://www.sciencedirect.com/science/article/pii/S1110016823008554},
   year = {2023}
}
@article{Li2023,
   abstract = {Accurate rainfall prediction remains a challenging problem because of the high volatility and complicated essence of atmospheric data. This study proposed a hybrid model (DSP) that combines the advantages of discrete wavelet transform (DWT), support vector regression (SVR), and Prophet to forecast rainfall data. First, the rainfall time series is decomposed into high-frequency and low-frequency subseries using discrete wavelet transform (DWT). The SVR and Prophet models are then used to predict high-frequency and low-frequency subsequences, respectively. Finally, the predicted rainfall is determined by summing the predicted values of each subsequence. A case study in China is conducted from 1 January 2014 to 30 June 2016. The results show that the DSP model provides excellent prediction, with RMSE, MAE, and R2 values of 6.17, 3.3, and 0.75, respectively. The DSP model yields higher prediction accuracy than the three baseline models considered, with the prediction accuracy ranking as follows: DSP > SSP > Prophet > SVR. In addition, the DSP model is quite stable and can achieve good results when applied to rainfall data from various climate types, with RMSEs ranging from 1.24 to 7.31, MAEs ranging from 0.52 to 6.14, and R2 values ranging from 0.62 to 0.75. The proposed model may provide a novel approach for rainfall forecasting and is readily adaptable to other time series predictions. © 2023 by the authors.},
   author = {D Li and J Ma and K Rao and X Wang and R Li and Y Yang and H Zheng},
   doi = {10.3390/w15101935},
   issn = {20734441 (ISSN)},
   issue = {10},
   journal = {Water (Switzerland)},
   keywords = {China,Climate models,Discrete wavelet transforms,Discrete-wavelet-transform,High frequency HF,High-low,Hybrid model,Learning systems,Machine learning,Machine-learning,Rain,Rainfall data,Rainfall time series prediction,Signal reconstruction,Support vector regressions,Time series,Time series prediction,Times series,Weather forecasting,discrete wavelet transform,hybrid model,hydrological modeling,machine learning,precipitation intensity,prediction,rainfall time series prediction,time series analysis,wavelet analysis},
   note = {Export Date: 17 February 2025; Cited By: 8; Correspondence Address: J. Ma; State Key Laboratory of Urban and Regional Ecology, Research Center for Eco-Environmental Sciences, Chinese Academy of Sciences, Beijing, 100085, China; email: jfma@rcees.ac.cn},
   publisher = {MDPI},
   title = {Prediction of Rainfall Time Series Using the Hybrid DWT-SVR-Prophet Model},
   volume = {15},
   url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85160673748&doi=10.3390%2fw15101935&partnerID=40&md5=f81c506e2540ba668839e0e2ff8bd49b},
   year = {2023}
}
@article{Yuan2022,
   abstract = {Reliable and accurate streamflow prediction plays a critical role in watershed water resources planning and management. We developed a new hybrid SWAT-WSVR model based on 12 hydrological sites in the Illinois River watershed (IRW), U.S., that integrated the Soil and Water Assessment Tool (SWAT) model with a Support Vector Regression (SVR) calibration method coupled with discrete wavelet transforms (DWT) to better support modeling watersheds with limited data availability. Wavelet components of the simulated streamflow from the SWAT-Calibration Uncertainty Procedure (SWAT-CUP) and precipitation time series were used as inputs to SVR to build a hybrid SWAT-WSVR. We examined the performance and potential of the SWAT-WSVR model and compared it with observations, SWAT-CUP, and SWAT-SVR using statistical metrics, Taylor diagrams, and hydrography. The results showed that the average of RMSE-observation’s standard deviation ratio (RSR), Nash–Sutcliffe efficiency (NSE), percent bias (PBIAS), and root mean square error (RMSE) from SWAT-WSVR is 0.02, 1.00, −0.15, and 0.27 m3 s−1 in calibration and 0.14, 0.98, −1.88, and 2.91 m3 s−1 in validation on 12 sites, respectively. Compared with the other two models, the proposed SWAT-WSVR model possessed lower discrepancy and higher accuracy. The rank of the overall performance of the three SWAT-based models during the whole study period was SWAT-WSVR > SWAT-SVR > SWAT-CUP. The developed SWAT-WSVR model supplies an additional calibration approach that can improve the accuracy of the SWAT streamflow simulation of watersheds with limited data. © 2022 by the authors.},
   author = {L Yuan and K J Forshay},
   doi = {10.3390/w14172649},
   issn = {20734441 (ISSN)},
   issue = {17},
   journal = {Water (Switzerland)},
   keywords = {Calibration,Discrete wavelet transforms,Discrete-wavelet-transform,Forecasting,Illinois,Illinois River,Illinois River watershed,Illinois river,Illinois river watershed,Limited data,Mean square error,Performance,Regression analysis,River watersheds,SWAT,Signal reconstruction,Soil and Water assessment tools,Stream flow,Streamflow prediction,Support vector regressions,United States,Watersheds,Wavelets transform,calibration,hydrological modeling,prediction,regression analysis,simulation,soil and water assessment tool,streamflow,streamflow prediction,support vector regression,watershed,wavelet analysis,wavelet transform},
   note = {Export Date: 17 February 2025; Cited By: 11; Correspondence Address: K.J. Forshay; U.S. Environmental Protection Agency, Center for Environmental Solutions and Emergency Response, Robert S. Kerr Environmental Research Center, Ada, 74820, United States; email: forshay.ken@epa.gov},
   publisher = {MDPI},
