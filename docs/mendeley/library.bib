   note = {Export Date: 17 February 2025; Cited By: 1; Correspondence Address: A. Wangwongchai; Department of Mathematics, Faculty of Science, King Mongkut’s University of Technology Thonburi (KMUTT), Bangkok, 10140, Thailand; email: angkool.wan@kmutt.ac.th},
   publisher = {American Institute of Physics},
   title = {A deep learning perspective on meteorological droughts prediction in the Mun River Basin, Thailand},
