   keywords = {Ardabil,Ardabil City,Iran,Outlier robust extreme learning machine,Rainfall,Simulation,Time series data,Wavelet,artificial intelligence,forecasting method,long-term change,machine learning,precipitation intensity,rainfall,wavelet analysis},
   note = {Export Date: 11 February 2025; Cited By: 9; Correspondence Address: S. Shabanlou; Department of Civil Engineering, Najafabad Branch, Islamic Azad University, Najafabad, Iran; email: saeid.shabanlou@gmail.com},
   pages = {2087-2100},
   publisher = {Springer Science and Business Media Deutschland GmbH},
