   journal = {Environmental Science and Pollution Research},
   keywords = {Agricultural Irrigation,Algorithms,Bagged tree,Era 5 reanalysis data,Evapotranspiration,Hakkari,Machine Learning,Machine learning,Solar Energy,Support vector regression,Turkey,Wavelet Analysis,Wavelet transform,algorithm,decomposition,evapotranspiration,irrigation (agriculture),machine learning,parallel computing,semiarid region,solar energy,vector autoregression,wavelet,wavelet analysis},
   note = {Export Date: 11 February 2025; Cited By: 14; Correspondence Address: O.M. Katipoğlu; Department of Civil Engineering, Erzincan Binali Yıldırım University, Erzincan, Turkey; email: okatipoglu@erzincan.edu.tr; CODEN: ESPLE},
