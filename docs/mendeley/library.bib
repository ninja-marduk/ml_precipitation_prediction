   issn = {2185310X (ISSN)},
   issue = {2},
   journal = {International Journal of Intelligent Engineering and Systems},
   keywords = {Hybrid FSGRNN (Feature Selection Gradient Recurrent Neural Network),Long short term memory- Convolutional neural network,Rainfall dataset,Rainfall forecasting,Recurrent neural network},
